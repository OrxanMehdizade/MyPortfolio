@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/stylesheet.css" />
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <script src="https://unpkg.com/typed.js@2.1.0/dist/typed.umd.js"></script>
</head>
<body>
    <header class="header">
        <a href="#" class="logo">Portfolio</a>
        <nav class="navbar">
            <a href="#" style="--i:1" class="active" >Home</a>
            <a href="#" style="--i:2" >About</a>
            <a href="#" style="--i:3" >Skill</a>
            <a href="#" style="--i:4" >Portfolio</a>
            <a href="#" style="--i:5" >Contact</a>
        </nav>
    </header>
    <section class="home">
        <div class="home-content">
            <h1>Hello, my name is Orkhan</h1>
            <h3>And I'm a <span class="text"></span></h3>
            <h6>As a Full Stack Developer, I have experience in both front-end and back-end technologies, allowing me to build dynamic and responsive web applications.</h6>
            <div class="home-sci">
                <a href="#" style="--i:7" ><i class='bx bxl-facebook'></i></a>
                <a href="#" style="--i:8" ><i class='bx bxl-instagram'></i></a>
                <a href="#" style="--i:9" ><i class='bx bxl-whatsapp'></i></a>
                <a href="#" style="--i:10" ><i class='bx bxl-linkedin'></i></a>
            </div>
            <a href="#" class="btn-box">More About Me</a>
            </div>
        </div>

    </section>

    <section class="about" id="about">
        <div class="about-img">
            <img src="/images/fto.jpg"/>
        </div>
    </section>



    <script src="~/js/portfolio.js"></script>
]
</body>
</html>
